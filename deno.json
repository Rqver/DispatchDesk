{
  "tasks": {
    "start": "deno run --env-file --unstable-cron --allow-env --allow-sys --allow-net --allow-read main.ts",
    "tailwind-dev": "deno run -A npm:@tailwindcss/cli -i public/assets/css/style.css -o ./public/assets/css/tailwind.css --watch",
    "tailwind-build": "deno run -A npm:@tailwindcss/cli -i public/assets/css/style.css -o ./public/assets/css/tailwind.css"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@tailwindcss/cli": "npm:@tailwindcss/cli@^4.1.4",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.16",
    "tailwindcss": "npm:tailwindcss@^4.1.4"
  },
  "nodeModulesDir": "auto"
}
